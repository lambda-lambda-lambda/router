{
  "name": "restfulApiHandler",
  "version": "0.0.1",
  "description": "Handler example with preconfigured routes and middleware.",
  "main": "app.js",
  "dependencies": {
    "lambda-lambda-lambda": "latest"
  },
  "devDependencies": {
    "eslint": "latest",
    "jsdoc": "latest",
    "swagger-jsdoc": "latest"
  },
  "scripts": {
    "genapi": "swagger-jsdoc -d src/openapi.js -o swagger.json src/app.js 'src/routes/**/*.js'",
    "gendoc": "jsdoc -d doc -r src",
    "lint": "eslint src/*.js"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
